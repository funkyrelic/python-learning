row1 = ["⬜️","️⬜️","️⬜️"]
row2 = ["⬜️","⬜️","️⬜️"]
row3 = ["⬜️️","⬜️️","⬜️️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")
position1 = str(position)[0]
position2 = str(position)[1]
map[int(position2)-1][int(position1)-1] = "X"
print(f"{row1}\n{row2}\n{row3}")
